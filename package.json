{
  "name": "polymer-starter-kit",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "eslint": "^3.12.0",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-html": "^1.7.0"
  },
  "scripts": {
    "lint": "eslint . --ext js,html --ignore-path .gitignore",
    "test": "npm run lint && polymer test",
    "deploy": "polymer build --bundle --js-compile --add-service-worker --sw-precache-config",
    "deployB": "polymer build --preset \"es5-bundled\" --js-compile --js-minify  --css-minify  --html-minify  --bundle --add-service-worker --add-push-manifest --sw-precache-config",
    "deployC": "polymer build --preset \"es5-bundled\" --js-compile --js-minify  --css-minify  --html-minify  --bundle"
  }
}
